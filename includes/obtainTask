            "phoneNumber": "18673125007",
            "wechatNumber": null,
            "source": "UPLOADED_BY_YOURSELF",
            "lastEmotion": "UNKNOWN",
            "customerLevel": "UNKNOWN",
            "gender": "UNKNOWN",
